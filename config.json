{
  "yt-dl-audio": {
    "dest": "~/Music",
    "options": {
      "postprocessors": [{
        "key": "FFmpegExtractAudio",
        "preferredcodec": "vorbis"
        }]
      }
    },
    "yt-dl-video": {
      "dest": "~/Sync/default",
      "options": {
        "postprocessors": [{
          "key": "FFmpegVideoConvertor",
          "preferedformat": "mp4"
        }]
      }
    }
  }
